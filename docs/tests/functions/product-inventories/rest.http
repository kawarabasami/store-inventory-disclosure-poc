post {{url}}/functions/v1/product-inventories
Authorization: Bearer {{anonKey}}
Content-Type: application/json

{
  "productInventories": [
    {
      "productId": "test-product1",
      "quantity": 10.5
    },
    {
      "productId": "test-product2",
      "quantity": 20
    }
  ]
}